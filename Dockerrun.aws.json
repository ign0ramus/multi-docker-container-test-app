{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "frontend",
			"image": "ign0ramus/multi-docker-container-test-app_frontend:latest",
			"hostname": "frontend",
			"essential": false,
			"memory": 128
		},
		{
			"name": "api",
			"image": "ign0ramus/multi-docker-container-test-app_api:latest",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "ign0ramus/multi-docker-container-test-app_worker:latest",
			"hostname": "worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "ign0ramus/multi-docker-container-test-app_nginx:latest",
			"hostname": "nginx",
			"essential": true,
			"memory": 128,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["frontend", "api"]
		}
	]
}
