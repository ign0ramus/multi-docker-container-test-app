{
	"AWSEBDockerrunVersion": 2,
	"containerDefenitions": [
		{
			"name": "frontend",
			"image": "ign0ramus/multi-docker-container-test-app_frontend",
			"hostname": "frontend",
			"essential": false
		},
		{
			"name": "api",
			"image": "ign0ramus/multi-docker-container-test-app_api",
			"hostname": "api",
			"essential": false
		},
		{
			"name": "worker",
			"image": "ign0ramus/multi-docker-container-test-app_worker",
			"hostname": "worker",
			"essential": false
		},
		{
			"name": "nginx",
			"image": "ign0ramus/multi-docker-container-test-app_nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["frontend", "api"]
		}
	]
}
